<% include partials/header %>

	 
	<div class="container">
			</br></br></br></br></br></br></br></br></br></br>
			<form action="/files/<%=file.filename%>?_method=PUT" method="POST">
				<div class="form-group ">
				  <label for="filename">Filename</label>
				  <input type="text" class="form-control" id="filename" name="filename" value="<%=file.filename%>">
				</div>
				<div class="form-group ">
				  <label for="trackname">Track Name</label>
				  <input type="text" class="form-control" id="trackname" name="trackname" value="<%=file.metadata.trackname%>">
				</div>
			 <div class="form-group ">
			   <label for="genre">Genre</label>
			   <input type="text" class="form-control" id="genre" name="genre" value="<%=file.metadata.genre%>">
			 </div>
			 <div class="form-group">
			   <label for="available">Available</label>
			   <input type="text" class="form-control" id="available" name="available" value="<%=file.metadata.available%>">
			 </div>
			 <div class="form-group">
			   <label for="length">Length </label>
			   <input type="text" class="form-control" id="length" name="length" value="<%=file.metadata.length%>">
			 </div>
			 <div class="form-group">
			   <label for="bpm">BPM </label>
			   <input type="text" class="form-control" id="bpm" name="bpm" value="<%=file.metadata.bpm%>">
			 </div> 
			 <div class="form-group">
			   <label for="id">ID </label>
			   <input type="text" class="form-control" id="id" name="id" value="<%=file.metadata.id%>">
			 </div> 
			<div class="form-group">
			   <label for="description">Description </label>
			   <input type="text" class="form-control" id="description" name="description" value="<%=file.metadata.description%>">
			 </div>
			 <button type="submit" class="btn btn-success">Submit</button>
				</form>
			
				<br>
			
			
				<a href="/tracks"><button class="btn btn-success">Back to Files</button></a>
	</div>

<% include partials/footer  %>